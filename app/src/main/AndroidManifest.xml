<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.logo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Logo">
        <activity
            android:name=".ui.main.view.master.MasterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.main.view.splash.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.main.view.search.FilterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.main.view.search.SearchInActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.main.view.NewsDetailActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>